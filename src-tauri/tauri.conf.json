{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "calculadora",
  "version": "0.1.0",
  "identifier": "com.sebac.calculadora",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Calculadora UTM / UF / CLP",
        "width": 550,
        "height": 650,
        "decorations": false,
        "transparent": false,
        "resizable": true,
        "minWidth": 400,
        "minHeight": 500
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
